{
  "name": "ngwebpack",
  "version": "1.0.0",
  "description": "<h1>ngWebpack<h1> <p>Angular-Webpack Boilerplate</p>",
  "main": "index.js",
  "scripts": {
    "dev-server": "nodemon --exec babel-node ./src/app/app.js",
    "dev-webpack": "webpack-dev-server --progress --color",
    "build": "webpack --progress --color && babel ./src/app/ --out-dir ./dist/app/",
    "prestart": "npm run build",
    "start": "node ./dist/app/app.js"
  },
